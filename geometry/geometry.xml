<?xml version="1.0" encoding="UTF-8"?>
<xml3d xmlns="http://www.w3.org/1999/xhtml">

	<asset id="sphere">
		<assetdata name="export">
			<int name="segments">80 60</int>
			<float name="phi">0 1</float>
			<float name="theta">0 1</float>
		</assetdata>
		<assetmesh compute="dataflow['dataflows.xml#gen-sphere']" includes="export"></assetmesh>
	</asset>

	<asset id="box">
		<assetdata name="export">
			<int name="segments">1 1 1</int>
			<float3 name="extend">2 2 2</float3>
		</assetdata>
		<assetmesh compute="dataflow['dataflows.xml#gen-box']" includes="export"></assetmesh>
	</asset>

</xml3d>